{
  "name": "spaak-assignment",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "waitForImages": "node -e \"setTimeout(() => process.exit(0), 10000)\"",
    "dev": "trap 'docker-compose down ; echo Stopped ; exit 0' INT; docker-compose up -d --wait && npm run waitForImages && npx prisma migrate dev && npx prisma db seed && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.2",
    "@trpc/react-query": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "lodash": "^4.17.21",
    "next": "15.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "15.0.2",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
